<h1>New Super Power Form</h1>

<ul>
  <% @power.errors.full_messages.each do |error| %>
    <li style="color: red;"><%= error %></li>
  <% end %>
</ul>

<%= form_for @power do |f| %>

	Super Power Name<br>
	<%= f.text_field :name %><br><br>

	Description<br>
	<%= f.text_area :description %><br><br>


	<%= f.submit 'Create' %>
<% end %>